# Portia SDK Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Optional: Portia Cloud (if using cloud storage)
# PORTIA_API_KEY=your-portia-api-key-here

# FastAPI Configuration
APP_NAME={{ cookiecutter.project_name }}
APP_VERSION={{ cookiecutter.version }}
DEBUG=false
LOG_LEVEL=info

# Server Configuration
HOST=0.0.0.0
PORT={{ cookiecutter.port }}

# Portia Configuration
PORTIA_LOG_LEVEL={{ cookiecutter.portia_log_level }}
PORTIA_STORAGE_CLASS={{ cookiecutter.portia_storage_class }}

# Optional: Additional LLM Providers
# ANTHROPIC_API_KEY=your-anthropic-api-key-here
# MISTRAL_API_KEY=your-mistral-api-key-here
# GOOGLE_API_KEY=your-google-api-key-here